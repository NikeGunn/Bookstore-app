# Production environment settings - used by Jenkins deployment pipeline
DEBUG=0
SECRET_KEY=replace_with_secure_key_in_jenkins_credentials
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1,your-aws-domain.com
API_SERVER_URL=https://your-aws-domain.com

# Database settings
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=replace_with_secure_password_in_jenkins_credentials
DATABASE_URL=postgresql://postgres:replace_with_secure_password_in_jenkins_credentials@db:5432/postgres

# CORS settings
CORS_ALLOWED_ORIGINS=https://your-frontend-domain.com
